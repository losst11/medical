<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <data noupdate="1">
        
        <record id="seq_medical_patient" model="ir.sequence">
            <field name="name">Medical Patient</field>
            <field name="code">medical.patient</field>
            <field name="prefix">IP</field>
            <field name="padding">3</field>
        </record>

        <record id="seq_medical_imaging_request" model="ir.sequence">
            <field name="name">Medical Imaging Request</field>
            <field name="code">medical.imaging.request</field>
            <field name="prefix">MI</field>
            <field name="padding">3</field>
            <!--<field name="company_id" eval="False"/>-->
        </record>

        <record id="seq_medical_patient_hospitalization" model="ir.sequence">
            <field name="name">Medical Patient Hospitalization</field>
            <field name="code">medical.patient.hospitalization</field>
            <field name="prefix">INPAC</field>
            <field name="padding">3</field>
            <!--<field name="company_id" eval="False"/>-->
        </record>

        <record id="seq_medical_appointment" model="ir.sequence">
            <field name="name">Medical Appointment</field>
            <field name="code">medical.appointment</field>
            <field name="prefix">AP</field>
            <field name="padding">3</field>
            <!--<field name="company_id" eval="False"/>-->
        </record>

        <record id="seq_medical_visit" model="ir.sequence">
            <field name="name">Medical Visit</field>
            <field name="code">medical.visit</field>
            <field name="prefix">VI</field>
            <field name="padding">3</field>
            <!--<field name="company_id" eval="False"/>-->
        </record>

    </data>
</odoo>
